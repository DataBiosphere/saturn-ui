{
  "name": "@terra-ui-packages/components",
  "version": "0.0.2",
  "private": false,
  "scripts": {
    "build": "yarn run build-es && yarn run build-cjs",
    "build-es": "tsc -p tsconfig.json && echo '{\"type\": \"module\"}' > lib/es/package.json",
    "build-cjs": "tsc -p tsconfig.cjs.json",
    "test": "jest"
  },
  "module": "./lib/es/index.js",
  "main": "./lib/cjs/index.js",
  "exports": {
    ".": {
      "import": "./lib/es/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "files": [
    "lib/cjs/**",
    "lib/es/**"
  ],
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@terra-ui-packages/test-utils": "*",
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.184",
    "@types/react": "^18.2.15",
    "jest": "^27.4.3",
    "react": "18.2.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": "18.2.0"
  }
}
