  "files": [
    "lib/cjs/**",
    "lib/es/**",
    "lib/types/**/*.d.ts"
  ],
  "devDependencies": {
    "@terra-ui-packages/build-utils": "^2.0.0",
    "@terra-ui-packages/test-utils": "^1.0.0",
    "@types/jest": "^28.1.8",
