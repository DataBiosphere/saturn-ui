    "pluralize": "^8.0.0",
    "prismjs": "^1.27.0",
    "prop-types": "^15.7.2",
    "qs": "^6.10.1",
    "react": "18.2.0",
    "react-collapse": "^5.1.0",
    "react-dom": "18.2.0",
    "react-draggable": "^4.4.4",
    "react-dropzone": "^11.4.0",
    "react-favicon": "^2.0.3",
    "react-focus-lock": "^2.9.5",
    "react-hyperscript-helpers": "^2.0.0",
    "react-modal": "^3.16.1",
    "react-notifications-component": "^4.0.1",
    "react-oidc-context": "^2.1.0",
    "react-onclickoutside": "^6.13.0",
    "react-paginating": "^1.4.0",
    "react-select": "^5.7.4",
    "react-simplemde-editor": "^5.0.2",
    "react-switch": "^6.1.0",
    "react-textarea-autosize": "^8.5.2",
    "react-transition-group": "^4.4.5",
    "react-virtualized": "patch:react-virtualized@npm:^9.22.5#./.yarn/patches/react-virtualized-9.22.5.patch",
    "use-memo-one": "^1.1.3",
    "uuid": "^8.3.2",
    "validate.js": "^0.13.1"
  },
  "scripts": {
    "analyze": "yarn build && source-map-explorer 'build/static/js/*.js' --gzip",
    "build": "yarn install && env $(yarn setenv) vite build && rm build/config.json",
    "lint": "eslint --fix --max-warnings=0 .",
    "optimize-image-svgs": "svgo --enable=inlineStyles,prefixIds --config '{ \"plugins\": [ { \"inlineStyles\": { \"onlyMatchedOnce\": false } }] }' --pretty -f src/images -r -p 1 --multipass",
    "preinstall": "node .hooks/check-engine-light.js",
    "setenv": "echo VITE_APP_VERSION=$(git rev-parse HEAD) VITE_APP_BUILD_TIMESTAMP=$(date -u \"+%s000\")",
    "test": "TZ=UTC jest",
    "check-types": "check-dts types/**/*.ts",
    "serve": "vite preview",
    "start": "env $(yarn setenv) vite",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@axe-core/react": "^4.6.1",
    "@babel/core": "^7.16.0",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.7.16",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-virtualized": "^9.21.22",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-jest": "^27.4.2",
    "babel-preset-react-app": "^10.0.1",
    "blob-polyfill": "^7.0.20220408",
    "browserslist": "^4.18.1",
    "camelcase": "^6.2.1",
    "check-dts": "^0.7.1",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-import-resolver-custom-alias": "^1.3.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "ignore": "^5.2.4",
    "jest": "^27.4.3",
    "jest-axe": "^6.0.0",
    "jest-fail-on-console": "^3.1.1",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.24",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.8.8",
    "source-map-explorer": "^2.5.2",
    "svgo": "^1.3.2",
    "typescript": "^4.9.5",
    "vite": "^4.3.9",
    "vite-plugin-svgr": "^3.2.0",
    "whatwg-fetch": "^3.6.2"
  },
  "resolutions": {
    "fbjs/ua-parser-js": "^1.0.33",
    "outdated-browser-rework/ua-parser-js": "^1.0.33"
  },
  "browserslist": [
    ">0.2%",
    "Firefox ESR",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependenciesMeta": {
    "@fortawesome/fontawesome-common-types": {
      "built": false
    },
    "@fortawesome/fontawesome-svg-core": {
      "built": false
    },
    "@fortawesome/free-regular-svg-icons": {
      "built": false
    },
    "@fortawesome/free-solid-svg-icons": {
      "built": false
    },
    "core-js": {
      "built": false
    },
    "core-js-pure": {
      "built": false
    }
  },
  "packageManager": "yarn@3.6.1",
  "workspaces": [
    "integration-tests"
  ],
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [],
    "modulePaths": [],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": false,
    "clearMocks": true
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "yarn eslint --max-warnings=0"
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-flexbugs-fixes": {},
      "postcss-preset-env": {
        "autoprefixer": {
          "flexbox": "no-2009"
        },
        "stage": 3
      },
      "postcss-normalize": {}
    }
  }
}
