    "core-js-pure": {
      "built": false
    }
  },
  "packageManager": "yarn@3.6.3",
  "workspaces": [
    "packages/*",
    "integration-tests"
  ],
  "postcss": {
    "plugins": {
      "postcss-flexbugs-fixes": {},
      "postcss-preset-env": {
        "autoprefixer": {
          "flexbox": "no-2009"
